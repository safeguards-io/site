/*******************************
     User Global Variables
*******************************/

/* Body Style */
@pageBackground: @pageGrey;

/* Text colors */
@primaryColor: @pink;
@primaryColorHover: @darkPink;
@secondaryColor: @grey;
@textColor: #595959;


/* Colors */
@pageGrey: #F7F7F7;
@pink : rgba(255,51,102,1);
@darkPink: darken(@pink, 10%);